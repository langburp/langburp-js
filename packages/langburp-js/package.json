{
  "name": "@langburp/langburp-js",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "private": false,
  "scripts": {
    "generate": "rm -rf ./src/langburpapi_client && openapi-generator-cli generate -i openapi-v3.1.0.json -g typescript-fetch -o ./src/langburpapi_client --additional-properties=useSingleRequestParameter=true,withSeparateModelsAndApi=true,apiPackage=server,modelPackage=model --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true",
    "build": "tsup src/index.ts --out-dir lib --dts --format cjs,esm --tsconfig tsconfig.json --no-splitting",
    "dev": "tsup src/index.ts --out-dir lib --dts --format cjs,esm --tsconfig tsconfig.json --no-splitting --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "langburp",
    "open-source",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/langburp/langburp-js"
  },
  "author": "Langburp",
  "license": "MIT",
  "homepage": "https://github.com/langburp/langburp-js",
  "bugs": {
    "url": "https://github.com/langburp/langburp-js/issues"
  },
  "description": "Langburp JS SDK",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.16.3",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.13.0",
    "typescript": "5.7.3",
    "tsc": "^2.0.4",
    "tsup": "^8.2.4"
  },
  "dependencies": {
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  }
}